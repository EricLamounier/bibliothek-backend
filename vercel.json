{
  "version": 2,
  "buildCommand": "echo Skip build",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.2.13",
      "memory": 1024,
      "maxDuration": 10
    },
    "api/cleanup-tokens.ts": {
      "runtime": "@vercel/node@3.2.13",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/cleanup-tokens",
      "schedule": "0 0 * * *"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/index" }
  ]
}
